{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vector Test Explorer",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "autoAttachChildProcesses": true,
      "outFiles": ["${workspaceFolder}/out/*.js"],
      "preLaunchTask": {
        "type": "npm",
        "script": "esbuild"
      }
    },
    {
      "name": "getEnviroData",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/VectorCAST/FOO",
      "env": {
        "PYTHONPATH": "C:/vcast/vc23sp5/python",
        "VECTORCAST_DIR": "c:/VCAST/vc21sp5"
      },
      "args": [
        "--mode=getEnviroData",
        "--clicast=C:/vcast/vc23sp5/clicast.exe",
        "--path=c:/RDS/VectorCAST/FOO/unitTests/FOO-VC23SP5"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "getCompletionData",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/testEditorInterface.py",
      "cwd": "C:/RDS/VectorCAST/CodeBasedTests/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc24/python",
        "VECTORCAST_DIR": "c:/VCAST/vc24sp2"
      },
      "args": [
        "choiceList-ct",
        "VMOCK_EXAMPLES",
        "// vmock vmock_examples myClass::operator== "
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "runTest",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/VectorCAST/FOO/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python"
      },
      "args": [
        "--mode=executeTest",
        "--clicast=C:/vcast/vc24sp2/clicast.exe",
        "--path=c:/RDS/VectorCAST/FOO/unitTests/FOO",
        "--test=\"FOO|foo.coded_tests_driver.fooTests.simple\""
      ],
      "console": "integratedTerminal"
    }
  ]
}
