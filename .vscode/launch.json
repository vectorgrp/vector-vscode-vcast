{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vector Test Explorer",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "autoAttachChildProcesses": true,
      "outFiles": ["${workspaceFolder}/out/*.js"],
      "preLaunchTask": {
        "type": "npm",
        "script": "esbuild"
      }
    },
    {
      "name": "getEnviroData",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/SandBox/VectorCAST",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python",
        "VECTORCAST_DIR": "c:/VCAST/vc21sp5"
      },
      "args": [
        "--mode=getEnviroData",
        "--clicast=C:/vcast/vc23sp2/clicast.exe",
        "--path=c:/RDS/SandBox/VectorCAST/examples/unitTests/FOO"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "getCoverageData",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/SandBox/VectorCAST/examples",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python"
      },
      "args": [
        "--mode=getCoverageData",
        "--clicast=C:/vcast/vc23sp2/clicast.exe",
        "--path=c:/RDS/SandBox/VectorCAST/examples/unitTests/FOO"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "getCompletion",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/testEditorInterface.py",
      "cwd": "C:/RDS/VectorCAST/testExplorer/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc23/python"
      },
      "args": [
        "choiceList",
        "C:\\RDS\\VectorCAST\\testExplorer\\unitTests\\MANAGER",
        "TEST.REQUIREMENT_KEY"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "runTest",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/SandBox/VCAST/sourceCode/cpp/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python"
      },
      "args": [
        "--mode=executeTest",
        "--path=c:/RDS/SandBox/VCAST-NEW/vcastTutorial/cpp/unitTests/DATABASE-MANAGER",
        "--test=\"DATABASE-MANAGER|manager.Manager::PlaceOrder.myThirdTest\""
      ],
      "console": "integratedTerminal"
    }
  ]
}
