{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vector Test Explorer",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "autoAttachChildProcesses": true,
      "outFiles": ["${workspaceFolder}/out/*.js"],
      "preLaunchTask": {
        "type": "npm",
        "script": "esbuild"
      }
    },
    {
      "name": "getEnviroData",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/SandBox/VectorCAST",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python",
        "VECTORCAST_DIR": "c:/VCAST/vc21sp5"
      },
      "args": [
        "--mode=getEnviroData",
        "--clicast=C:/vcast/vc23sp2/clicast.exe",
        "--path=c:/RDS/SandBox/VectorCAST/examples/unitTests/FOO"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "runTest",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/SandBox/VCAST/sourceCode/cpp/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python"
      },
      "args": [
        "--mode=executeTest",
        "--path=c:/RDS/SandBox/VCAST-NEW/vcastTutorial/cpp/unitTests/DATABASE-MANAGER",
        "--test=\"DATABASE-MANAGER|manager.Manager::PlaceOrder.myThirdTest\""
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "runTest",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/SandBox/VCAST/sourceCode/cpp/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python"
      },
      "args": [
        "--mode=executeTest",
        "--path=c:/RDS/SandBox/VCAST-NEW/vcastTutorial/cpp/unitTests/DATABASE-MANAGER",
        "--test=\"DATABASE-MANAGER|manager.Manager::PlaceOrder.myThirdTest\""
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "rebuild",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/vTestInterface.py",
      "cwd": "C:/RDS/VectorCAST/FOO/unitTests",
      "env": {
        "PYTHONPATH": "C:/vcast/vc22sp7/python"
      },
      "args": [
        "--mode=rebuild",
        "--clicast=C:/vcast/vc24/clicast.exe",
        "--path=C:/RDS/VectorCAST/FOO/unitTests/FOO",
        "--options={\"ENVIRO.COVERAGE_TYPE\": \"Statement\"}"
      ],
      "console": "internalConsole"
    }
  ]
}
