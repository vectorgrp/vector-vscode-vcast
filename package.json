    "configuration": [
      {
        "id": "general",
        "title": "General",
        "order": 1,
        "properties": {
          "vectorcastTestExplorer.vectorcastInstallationLocation": {
            "order": 1,
            "type": "string",
            "description": "Path to the VectorCAST installation, used if VectorCAST is not found on the system PATH"
          },
          "vectorcastTestExplorer.useDataServer": {
            "type": "boolean",
            "order": 2,
            "default": false,
            "description": "Provides faster interaction with VectorCAST environments"
          },
          "vectorcastTestExplorer.automaticallyUpdateManageProject": {
            "type": "boolean",
            "order": 2,
            "default": true,
            "description": "Automatically update the VectorCAST project when changes are made. If disabled, the user must manually update the project by right-clicking on a node in the Testing pane and selecting 'Update Project Level'"
          },
          "vectorcastTestExplorer.unitTestLocation": {
            "type": "string",
            "order": 3,
            "default": "./unitTests",
            "description": "The folder path for storing unit tests, can be absolute or relative to the file location"
          },
          "vectorcastTestExplorer.configurationLocation": {
            "type": "string",
            "order": 4,
            "default": "",
            "description": "The file path to an existing VectorCAST configuration that should be used when creating new environments.  If a value is not specified, the configuration editor will be launched"
          },
          "vectorcastTestExplorer.showReportOnExecute": {
            "type": "boolean",
            "order": 5,
            "default": true,
            "description": "Display the VectorCAST execution report after test run"
          },
          "vectorcastTestExplorer.decorateExplorer": {
            "type": "boolean",
            "order": 6,
            "default": true,
            "description": "Decorate files that have coverage in the File Explorer pane"
          },
          "vectorcastTestExplorer.verboseLogging": {
            "type": "boolean",
            "order": 7,
            "default": false,
            "description": "Log internal extension details"
          },
          "vectorcastTestExplorer.decomposeRequirements": {
            "type": "boolean",
            "order": 8,
            "default": true,
            "description": "Decompose requirements when generating tests"
          },
          "vectorcastTestExplorer.generateHighLevelRequirements": {
            "type": "boolean",
            "order": 9,
            "default": false,
            "description": "Generate high-level requirements in addition to low-level requirements."
          },
          "vectorcastTestExplorer.outputDebugInfo": {
            "type": "boolean",
            "order": 10,
            "default": true,
            "description": "Output debug information to the console"
          },
          "vectorcastTestExplorer.requirementsLanguage": {
            "type": "string",
            "order": 11,
            "default": "en",
            "enum": [
              "af", "sq", "am", "ar", "hy", "as", "az", "be", "bn", "bs", "bg", "my", "ch", "hr", "cs", "da", "nl", "en", "et", "tl", "fi", "fr", "ka", "de", "gu", "ha", "he", "hi", "hu", "is", "ig", "id", "ga", "it", "jp", "kn", "kk", "km", "ko", "ky", "lo", "lv", "lt", "mk", "ms", "ml", "mt", "mn", "me", "ne", "no", "or", "fa", "pl", "pt", "pa", "ro", "ru", "sr", "si", "sk", "sl", "es", "sw", "sv", "tg", "ta", "te", "th", "tr", "uk", "ur", "uz", "vi", "cy", "xh", "yo", "zu"
            ],
            "enumItemLabels": [
              "Afrikaans", "Albanian", "Amharic", "Arabic", "Armenian", "Assamese", "Azerbaijani", "Belarusian", "Bengali", "Bosnian", "Bulgarian", "Burmese", "Chinese", "Croatian", "Czech", "Danish", "Dutch", "English", "Estonian", "Filipino", "Finnish", "French", "Georgian", "German", "Gujarati", "Hausa", "Hebrew", "Hindi", "Hungarian", "Icelandic", "Igbo", "Indonesian", "Irish", "Italian", "Japanese", "Kannada", "Kazakh", "Khmer", "Korean", "Kyrgyz", "Lao", "Latvian", "Lithuanian", "Macedonian", "Malay", "Malayalam", "Maltese", "Mongolian", "Montenegrin", "Nepali", "Norwegian", "Odia", "Persian", "Polish", "Portuguese", "Punjabi", "Romanian", "Russian", "Serbian", "Sinhala", "Slovak", "Slovenian", "Spanish", "Swahili", "Swedish", "Tajik", "Tamil", "Telugu", "Thai", "Turkish", "Ukrainian", "Urdu", "Uzbek", "Vietnamese", "Welsh", "Xhosa", "Yoruba", "Zulu"
            ],
            "description": "Language for requirements and test generation responses"
          }
        }
      },
      {
        "id": "build",
        "title": "Environment Build",
        "order": 2,
        "properties": {
          "vectorcastTestExplorer.build.coverageKind": {
            "type": "string",
            "enum": [
              "None",
              "Statement",
              "Branch",
              "Statement+Branch",
              "MCDC",
              "Statement+MCDC"
            ],
            "enumDescriptions": [
              "Coverage disabled",
              "Statement coverage",
              "Branch coverage",
              "Statement and branch coverage",
              "MC/DC coverage",
              "Statement and MC/DC coverage"
            ],
            "order": 1,
            "default": "Statement",
            "description": "Sets the coverage kind when building a new environments"
          },
          "vectorcastTestExplorer.build.enableCodedTesting": {
            "type": "boolean",
            "order": 2,
            "default": false,
            "description": "Enables coded testing when building a new environment"
          }
        }
      }
    ],
