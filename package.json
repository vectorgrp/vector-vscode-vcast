{
  "name": "vectorcasttestexplorer",
  "displayName": "VectorCAST Test Explorer",
  "description": "VectorCAST Test Explorer for VS Code",
  "version": "1.0.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vectorgrp/vector-vscode-vcast"
  },
  "sponsor": {
    "url": "https:vector.com"
  },
  "icon": "resources/vector-icon.png",
  "author": "Vector Informatik GmbH",
  "publisher": "vectorgroup",
  "categories": [
    "Testing",
    "Snippets"
  ],
  "bugs": {
    "url": "https://github.com/vectorgrp/vector-vscode-vcast/issues"
  },
  "qna": "https://github.com/vectorgrp/vector-vscode-vcast/discussions/categories/q-n-a",
  "keywords": [
    "vectorcast",
    "vector",
    "unittesting",
    "c++",
    "test",
    "testing"
  ],
  "main": "out/extension.js",
  "activationEvents": [
    "workspaceContains:**/UNITDATA.VCD"
  ],
  "contributes": {
    "viewsWelcome": [
      {
        "view": "testing",
        "when": "!vectorcastTestExplorer.configured",
        "contents": "[Configure VectorCAST Tests](command:vectorcastTestExplorer.configure)"
      }
    ],
    "commands": [
      {
        "command": "vectorcastTestExplorer.configure",
        "category": "VectorCAST Test Explorer",
        "title": "Configure"
      },
      {
        "command": "vectorcastTestExplorer.showSettings",
        "title": "Open settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "vectorcastTestExplorer.toggleLog",
        "category": "VectorCAST Test Explorer",
        "title": "View Message Panel",
        "icon": "$(output)"
      },
      {
        "command": "vectorcastTestExplorer.openVCAST",
        "category": "VectorCAST Test Explorer",
        "title": "Open Environment"
      },
      {
        "command": "vectorcastTestExplorer.openVCASTFromVce",
        "category": "VectorCAST Test Explorer",
        "title": "Open VectorCAST Environment"
      },
      {
        "command": "vectorcastTestExplorer.rebuildEnviro",
        "category": "VectorCAST Test Explorer",
        "title": "Re-Build Environment"
      },
      {
        "command": "vectorcastTestExplorer.deleteEnviro",
        "category": "VectorCAST Test Explorer",
        "title": "Delete Environment"
      },
      {
        "command": "vectorcastTestExplorer.newEnviroVCAST",
        "category": "VectorCAST Test Explorer",
        "title": "Create VectorCAST Environment"
      },
      {
        "command": "vectorcastTestExplorer.buildEnviroFromEnv",
        "category": "VectorCAST Test Explorer",
        "title": "Build VectorCAST Environment"
      },
      {
        "command": "vectorcastTestExplorer.addLaunchConfiguration",
        "category": "VectorCAST Test Explorer",
        "title": "VectorCAST: Add Launch Configuration"
      },
      {
        "command": "vectorcastTestExplorer.addIncludePath",
        "category": "VectorCAST Test Explorer",
        "title": "VectorCAST: Add Coded Test Include Path"
      },
      {
        "command": "vectorcastTestExplorer.addSettingsFileFilter",
        "category": "VectorCAST Test Explorer",
        "title": "VectorCAST: Add Filter for Environment Files"
      },
      {
        "command": "vectorcastTestExplorer.coverage",
        "category": "VectorCAST Test Explorer",
        "title": "Toggle Coverage Annotations"
      },
      {
        "command": "vectorcastTestExplorer.viewResults",
        "category": "VectorCAST Test Explorer",
        "title": "View Test Results"
      },
      {
        "command": "vectorcastTestExplorer.createTestScript",
        "category": "VectorCAST Test Explorer",
        "title": "New Test Script"
      },
      {
        "command": "vectorcastTestExplorer.addCodedTests",
        "category": "VectorCAST Test Explorer",
        "title": "Add Existing Coded Test File"
      },
      {
        "command": "vectorcastTestExplorer.generateCodedTests",
        "category": "VectorCAST Test Explorer",
        "title": "Generate New Coded Test File"
      },
      {
        "command": "vectorcastTestExplorer.removeCodedTests",
        "category": "VectorCAST Test Explorer",
        "title": "Remove Coded Tests"
      },
      {
        "command": "vectorcastTestExplorer.insertBasisPathTests",
        "category": "VectorCAST Test Explorer",
        "title": "Insert Basis Path Tests"
      },
      {
        "command": "vectorcastTestExplorer.insertATGTests",
        "category": "VectorCAST Test Explorer",
        "title": "Insert ATG Tests"
      },
      {
        "command": "vectorcastTestExplorer.insertBasisPathTestsFromEditor",
        "category": "VectorCAST Test Explorer",
        "title": "Generate Basis Path Tests"
      },
      {
        "command": "vectorcastTestExplorer.insertATGTestsFromEditor",
        "category": "VectorCAST Test Explorer",
        "title": "Generate ATG Tests"
      },
      {
        "command": "vectorcastTestExplorer.deleteTest",
        "category": "VectorCAST Test Explorer",
        "title": "Delete Tests"
      },
      {
        "command": "vectorcastTestExplorer.editTestScript",
        "category": "VectorCAST Test Explorer",
        "title": "Edit Test Script"
      },
      {
        "command": "vectorcastTestExplorer.editCodedTest",
        "category": "VectorCAST Test Explorer",
        "title": "Edit Coded Test"
      },
      {
        "command": "vectorcastTestExplorer.loadTestScript",
        "category": "VectorCAST Test Explorer",
        "title": "Load Test Script into Environment"
      },
      {
        "command": "vectorcastTestExplorer.debugEnviroPath",
        "category": "VectorCAST Test Explorer",
        "title": "Path for environment containing test being debugged"
      },
      {
        "command": "vectorcastTestExplorer.debugProgramPath",
        "category": "VectorCAST Test Explorer",
        "title": "Path to executable being debugged"
      },
      {
        "command": "vectorcastTestExplorer.createTestScriptFromEditor",
        "category": "VectorCAST Test Explorer",
        "title": "New Test Script"
      },
      {
        "command": "vectorcastTestExplorer.setDefaultConfigFile",
        "category": "VectorCAST Test Explorer",
        "title": "Set as VectorCAST Configuration File"
      }
    ],
    "keybindings": [
      {
        "command": "vectorcastTestExplorer.coverage",
        "key": "shift+ctrl+c",
        "when": "editorTextFocus"
      },
      {
        "command": "vectorcastTestExplorer.toggleLog",
        "key": "shift+ctrl+v"
      }
    ],
    "configuration": [
      {
        "id": "general",
        "title": "General",
        "order": 1,
        "properties": {
          "vectorcastTestExplorer.vectorcastInstallationLocation": {
            "order": 1,
            "type": "string",
            "description": "Path to the VectorCAST installation, used if VectorCAST is not found on the system PATH"
          },
          "vectorcastTestExplorer.unitTestLocation": {
            "type": "string",
            "order": 2,
            "default": "./unitTests",
            "description": "The folder path for storing unit tests, can be absolute or relative to the file location"
          },
          "vectorcastTestExplorer.configurationLocation": {
            "type": "string",
            "order": 3,
            "default": "",
            "description": "The file path to an existing VectorCAST configuration that should be used when creating new environments.  If a value is not specified, the configuration editor will be launched"
          },
          "vectorcastTestExplorer.showReportOnExecute": {
            "type": "boolean",
            "order": 4,
            "default": true,
            "description": "Display the VectorCAST execution report after test run"
          },
          "vectorcastTestExplorer.decorateExplorer": {
            "type": "boolean",
            "order": 5,
            "default": true,
            "description": "Decorate files that have coverage in the File Explorer pane"
          },
          "vectorcastTestExplorer.verboseLogging": {
            "type": "boolean",
            "order": 6,
            "default": false,
            "description": "Log internal extension details"
          }
        }
      },
      {
        "id": "build",
        "title": "Environment Build",
        "order": 2,
        "properties": {
          "vectorcastTestExplorer.build.coverageKind": {
            "type": "string",
            "enum": [
              "None",
              "Statement",
              "Statement+Branch",
              "Statement+MCDC"
            ],
            "enumDescriptions": [
              "Coverage disabled",
              "Statement coverage",
              "Statement and branch coverage",
              "Statement and MC/DC coverage"
            ],
            "order": 1,
            "default": "Statement",
            "description": "Sets the coverage kind when building a new environments"
          },
          "vectorcastTestExplorer.build.enableCodedTesting": {
            "type": "boolean",
            "order": 2,
            "default": false,
            "description": "Enables coded testing when building a new environment"
          }
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vectorcastTestExplorer.configure"
        },
        {
          "command": "vectorcastTestExplorer.toggleLog",
          "when": "vectorcastTestExplorer.configured"
        },
        {
          "command": "vectorcastTestExplorer.openVCAST",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.openVCASTFromVce",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.rebuildEnviro",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.deleteEnviro",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.newEnviroVCAST",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.buildEnviroFromEnv",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.addLaunchConfiguration",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.addIncludePath",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.addSettingsFileFilter",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.coverage",
          "when": "vectorcastTestExplorer.configured"
        },
        {
          "command": "vectorcastTestExplorer.viewResults",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.createTestScript",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.addCodedTests",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.generateCodedTests",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.removeCodedTests",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.insertBasisPathTests",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.insertATGTests",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.insertBasisPathTestsFromEditor",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.insertATGTestsFromEditor",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.deleteTest",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.editTestScript",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.editCodedTest",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.loadTestScript",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.debugEnviroPath",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.debugProgramPath",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.createTestScriptFromEditor",
          "when": "never"
        },
        {
          "command": "vectorcastTestExplorer.setDefaultConfigFile",
          "when": "never"
        }
      ],
      "view/title": [
        {
          "command": "vectorcastTestExplorer.toggleLog",
          "when": "view=~/^.*test.*$/",
          "group": "navigation@2"
        },
        {
          "command": "vectorcastTestExplorer.showSettings",
          "when": "view=~/^.*test.*$/",
          "group": "navigation@3"
        }
      ],
      "explorer/context": [
        {
          "command": "vectorcastTestExplorer.newEnviroVCAST",
          "when": "resourceLangId == cpp || resourceLangId == c",
          "group": "2_workspace"
        },
        {
          "command": "vectorcastTestExplorer.addLaunchConfiguration",
          "when": "vectorcastTestExplorer.configured && resourceFilename==launch.json",
          "group": "7_modification"
        },
        {
          "command": "vectorcastTestExplorer.addIncludePath",
          "when": "vectorcastTestExplorer.codedTestingAvailable && resourceFilename==c_cpp_properties.json",
          "group": "7_modification"
        },
        {
          "command": "vectorcastTestExplorer.addSettingsFileFilter",
          "when": "vectorcastTestExplorer.configured && resourceFilename==settings.json",
          "group": "7_modification"
        },
        {
          "command": "vectorcastTestExplorer.buildEnviroFromEnv",
          "when": "vectorcastTestExplorer.configured && resourceExtname==.env",
          "group": "2_workspace"
        },
        {
          "command": "vectorcastTestExplorer.openVCASTFromVce",
          "when": "vectorcastTestExplorer.configured && resourceExtname==.vce",
          "group": "2_workspace"
        },
        {
          "command": "vectorcastTestExplorer.setDefaultConfigFile",
          "when": "vectorcastTestExplorer.configured && resourceFilename==CCAST_.CFG",
          "group": "2_workspace"
        }
      ],
      "editor/context": [
        {
          "command": "vectorcastTestExplorer.loadTestScript",
          "when": "vectorcastTestExplorer.configured && resourceExtname==.tst"
        },
        {
          "command": "vectorcastTestExplorer.addLaunchConfiguration",
          "when": "vectorcastTestExplorer.configured && resourceFilename==launch.json"
        },
        {
          "command": "vectorcastTestExplorer.addIncludePath",
          "when": "vectorcastTestExplorer.codedTestingAvailable && resourceFilename==c_cpp_properties.json"
        }
      ],
      "editor/lineNumber/context": [
        {
          "submenu": "vectorcastTestExplorer.editorSubMenu",
          "when": "vectorcastTestExplorer.configured && editorLineNumber in vectorcastTestExplorer.testableLineList",
          "group": "vcast"
        }
      ],
      "testing/item/context": [
        {
          "submenu": "vectorcastTestExplorer.vcastsubmenu",
          "group": "vcast"
        }
      ],
      "vectorcastTestExplorer.editorSubMenu": [
        {
          "command": "vectorcastTestExplorer.createTestScriptFromEditor",
          "group": "vcast@1"
        },
        {
          "command": "vectorcastTestExplorer.insertBasisPathTestsFromEditor",
          "group": "vcast@2"
        },
        {
          "command": "vectorcastTestExplorer.insertATGTestsFromEditor",
          "group": "vcast@3"
        }
      ],
      "vectorcastTestExplorer.vcastsubmenu": [
        {
          "command": "vectorcastTestExplorer.openVCAST",
          "group": "vcast@1",
          "when": "testId =~ /^vcast:.*$/ && testId in vectorcastTestExplorer.vcastEnviroList"
        },
        {
          "command": "vectorcastTestExplorer.rebuildEnviro",
          "group": "vcast@2",
          "when": "testId =~ /^vcast:.*$/ && testId in vectorcastTestExplorer.vcastEnviroList"
        },
        {
          "command": "vectorcastTestExplorer.deleteEnviro",
          "group": "vcast@3",
          "when": "testId =~ /^vcast:.*$/ && testId in vectorcastTestExplorer.vcastEnviroList"
        },
        {
          "command": "vectorcastTestExplorer.editTestScript",
          "group": "vcast@4",
          "when": "testId =~ /^vcast:.*$/ && !(testId =~ /.*coded_tests_driver.*/)"
        },
        {
          "command": "vectorcastTestExplorer.createTestScript",
          "group": "vcast@5",
          "when": "testId =~ /^vcast:.*$/ && testId not in vectorcastTestExplorer.vcastEnviroList && !(testId =~ /.*coded_tests_driver.*/)"
        },
        {
          "command": "vectorcastTestExplorer.addCodedTests",
          "group": "vcast@4",
          "when": "testId =~ /^vcast:.*$/ && testId =~ /.*coded_tests_driver$/ &&  testId not in vectorcastTestExplorer.vcastHasCodedTestsList"
        },
        {
          "command": "vectorcastTestExplorer.generateCodedTests",
          "group": "vcast@5",
          "when": "testId =~ /^vcast:.*$/ && testId =~ /.*coded_tests_driver$/ &&  testId not in vectorcastTestExplorer.vcastHasCodedTestsList"
        },
        {
          "command": "vectorcastTestExplorer.removeCodedTests",
          "group": "vcast@6",
          "when": "testId =~ /^vcast:.*$/ && testId =~ /.*coded_tests_driver$/ && testId in vectorcastTestExplorer.vcastHasCodedTestsList"
        },
        {
          "command": "vectorcastTestExplorer.editCodedTest",
          "group": "vcast@7",
          "when": "testId =~ /^vcast:.*$/ && testId not in vectorcastTestExplorer.vcastEnviroList && testId =~ /.*coded_tests_driver.+/"
        },
        {
          "command": "vectorcastTestExplorer.insertBasisPathTests",
          "group": "vcast@6",
          "when": "testId =~ /^vcast:.*$/ && !(testId =~ /^.*<<COMPOUND>>.*$/) && !(testId =~ /^.*<<INIT>>.*$/) && !(testId =~ /.*coded_tests_driver.*/)"
        },
        {
          "command": "vectorcastTestExplorer.insertATGTests",
          "group": "vcast@7",
          "when": "testId =~ /^vcast:.*$/ && vectorcastTestExplorer.atgAvailable && !(testId =~ /^.*<<COMPOUND>>.*$/) && !(testId =~ /^.*<<INIT>>.*$/) && !(testId =~ /.*coded_tests_driver.*/)"
        },
        {
          "command": "vectorcastTestExplorer.deleteTest",
          "group": "vcast@8",
          "when": "testId =~ /^vcast:.*$/ && !(testId =~ /.*coded_tests_driver.*/)"
        },
        {
          "command": "vectorcastTestExplorer.viewResults",
          "group": "vcast@9",
          "when": "testId =~ /^vcast:.*$/ && testId not in vectorcastTestExplorer.vcastEnviroList"
        }
      ]
    },
    "submenus": [
      {
        "label": "VectorCAST",
        "id": "vectorcastTestExplorer.vcastsubmenu"
      },
      {
        "label": "VectorCAST",
        "id": "vectorcastTestExplorer.editorSubMenu"
      }
    ],
    "languages": [
      {
        "id": "vcast.tst",
        "aliases": [
          "VectorCAST Test Script",
          "TST",
          "tst"
        ],
        "extensions": [
          ".tst"
        ],
        "configuration": "./syntax/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vcast.tst",
        "scopeName": "source.vcast.tst",
        "path": "./syntax/vcast.tst.grammar.json"
      }
    ],
    "snippets": [
      {
        "language": "vcast.tst",
        "path": "./syntax/tst.json"
      },
      {
        "language": "c",
        "path": "./syntax/mock.json"
      },
      {
        "language": "cpp",
        "path": "./syntax/mock.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify && npm run esbuild-server -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --main-fields=module,main",
    "esbuild-server": "esbuild ./server/server.ts --bundle --outfile=out/server.js --external:vscode --format=cjs --platform=node",
    "esbuild-watch": "npm-run-all --parallel esbuild-base esbuild-server -- --sourcemap --watch",
    "esbuild": "npm run esbuild-base -- --sourcemap && npm run esbuild-server -- --sourcemap",
    "package": "tsc --project tsconfig.json && vsce package",
    "publish": "vsce publish",
    "test": "tsc --project tsconfig_test.json && vitest run --reporter verbose --coverage",
    "gh-test": "vitest run --reporter=junit --outputFile=./test-output.xml --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-v8": "^0.32.2",
    "@vscode/test-electron": "^1.6.2",
    "esbuild": "^0.14.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "inject-markdown": "^1.5.0",
    "mocha": "^9.1.3",
    "prettier": "^3.3.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.7.1",
    "unzipper": "^0.10.11",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@vscode/vsce": "^2.32.0",
    "crc-32": "^1.2.0",
    "glob": "^7.1.7",
    "hasbin": "^1.2.3",
    "jsonc-parser": "^3.2.1",
    "local": "^0.3.3",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "tslib": "^1.9.3",
    "vscode-languageclient": "^5.2.1",
    "vscode-languageserver": "^5.2.1",
    "xo": "^0.58.0"
  },
  "extensionDependencies": [],
  "engines": {
    "vscode": "^1.78.0"
  }
}