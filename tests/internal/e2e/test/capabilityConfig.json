{
    "maxInstances": 1,
    "browserName": "vscode",
    "browserVersion": "1.78.0",
    "acceptInsecureCerts": true,
    "wdio:vscodeOptions": {
        "vscodeArgs": {
          "disableExtensions" : true,
          "extensionsDir" : "",
          "local-history.enabled": false
        },
        "verboseLogging": false,
        "userSettings": {
            "editor.fontSize": 18,
            "terminal.integrated.fontSize": 12,
            "window.zoomLevel" : -5
        },
        "vscodeProxyOptions":{
            "enable": true,
            "port": 5678,
            "connectionTimeout": 30000,
            "commandTimeout": 30000
        }
    }
}